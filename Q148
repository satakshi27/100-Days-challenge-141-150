Q148:Take two structs as input and check if they are identical.

#include <stdio.h>
#include <string.h>

typedef struct
{
    char name[50];
    int roll_no;
    float marks;
} Student;

int areStudentsIdentical(Student s1, Student s2)
{
    if (strcmp(s1.name, s2.name) != 0) 
        return 0;
    if (s1.roll_no != s2.roll_no)      
        return 0;
    if (s1.marks != s2.marks)
        return 0;
    return 1; 

int main() 
{
    Student s1, s2;
    printf("Enter details of student 1:\n");
    printf("Name: ");
    scanf(" %[^\n]", s1.name);
    printf("Roll Number: ");
    scanf("%d", &s1.roll_no);
    printf("Marks: ");
    scanf("%f", &s1.marks);
    printf("\nEnter details of student 2:\n");
    printf("Name: ");
    scanf(" %[^\n]", s2.name);
    printf("Roll Number: ");
    scanf("%d", &s2.roll_no);
    printf("Marks: ");
    scanf("%f", &s2.marks);

    if (areStudentsIdentical(s1, s2)) {
        printf("\nThe two students are identical.\n");
    } else {
        printf("\nThe two students are NOT identical.\n");
    }

    return 0;
}
